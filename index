            ];

            function displayTools(tools) {
                const list = document.getElementById('aiToolsList');
                list.innerHTML = '';

                tools.forEach(tool => {
                    const toolElement = document.createElement('div');
                    toolElement.className = 'tool';

                    toolElement.innerHTML = `
                        <h3>${tool.name}</h3>
                        <p>${tool.definition}</p>
                        <a href="${tool.link}" target="_blank" class="website-link">Website</a>
                        <div class="tags">${tool.tags.map(tag => `<span>${tag}</span>`).join('')}</div>
                        <div class="videos">${tool.videos.map(video => `<a href="${video.url}" target="_blank">${video.courseName} (at ${video.timestamp})</a>`).join(', ')}</div>
                    `;

                    list.appendChild(toolElement);
                });
            }

            function filterTools(query) {
                const filteredTools = aiTools.filter(tool => 
                    tool.name.toLowerCase().includes(query.toLowerCase()) ||
                    tool.tags.some(tag => tag.toLowerCase().includes(query.toLowerCase()))
                );
                displayTools(filteredTools);
            }

            document.getElementById('searchBox').addEventListener('input', (e) => {
                filterTools(e.target.value);
            });

            displayTools(aiTools);
        });
    </script>
</body>
</html>
